{
    "name": "nordkirche/nkc-event",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Extension zur Darstellung von Veranstaltungen aus der NAPI",
    "license": "GPL-2.0",
    "author": [
        {
            "name": "Holger McCloy",
            "role": "Developer",
            "company": "netzleuchten GmbH"
        }
    ],
    "require": {
        "typo3/cms-core": "^12.4",
        "fluidtypo3/vhs": "^6.1 || ^7.0"
    },
    "config": {
        "bin-dir": ".build/bin",
        "discard-changes": true,
        "optimize-autoloader": true,
        "vendor-dir": ".build/vendor"
    },
    "autoload": {
        "psr-4": {
            "Nordkirche\\NkcEvent\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "nkc_event",
            "app-dir": ".build",
            "web-dir": ".build/public"
        }
    },
    "require-dev": {
        "typo3/coding-standards": "^0.7"
    }
}
